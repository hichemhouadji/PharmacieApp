<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pharmaciehichem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="PharmacieHichem"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name="com.example.pharmaciehichem.position_pharmacie_activity"
            android:label="@string/title_activity_position_pharmacie_activity"></activity>
        <activity android:name="com.example.pharmaciehichem.client_activity" />
        <activity android:name="com.example.pharmaciehichem.login_activity" />
        <activity android:name="com.example.pharmaciehichem.registration_activity" />
        <activity android:name="com.example.pharmaciehichem.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>